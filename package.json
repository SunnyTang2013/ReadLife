{
  "name": "scorch-frontend",
  "description": "Scorch Frontend, powered by React.js",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src/main/frontend/**/*.js --max-warnings 10 -f html -o build/reports/eslint/index.html",
    "lint:autofix": "eslint src/main/frontend/**/*.js --fix --max-warnings 10 -f html -o build/reports/eslint/index.html",
    "start": "webpack server --config webpack.dev.js --progress --color --stats --progress --watch-options-stdin",
    "build": "webpack --config webpack.dev.js --progress --color --stats --progress --watch-options-stdin",
    "build:watch": "webpack --config webpack.dev.js --watch --stats",
    "build:prod": "webpack --config webpack.prod.js --progress --color",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "author": "HSBC",
  "license": "ISC",
